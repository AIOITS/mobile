{
  "expo": {
    "name": "aioits",
    "slug": "aioits",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/icon-aioits.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow AIOITS to access your camera."
        }
      ],
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf"
    ],
    "splash": {
      "image": "./assets/logo/aioits.png",
      "resizeMode": "contain",
      "backgroundColor": "#f1f9fd"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/logo-aioits.png",
        "backgroundColor": "#f1f9fd"
      },
      "package": "com.nizz14.aioits",
      "permissions": ["ACCESS_BACKGROUND_LOCATION"]
    },
    "web": {
      "favicon": "./logo/logo-aioits.png"
    },
    "extra": {
      "eas": {
        "projectId": "aefeb37e-b415-4640-aef2-33e44e5c511b"
      }
    }
  }
}

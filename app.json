{
  "expo": {
    "name": "aioits",
    "slug": "aioits",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f1f9fd"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/logo-blue.png",
        "backgroundColor": "#f1f9fd"
      }
    },
    "web": {
      "favicon": "./logo/logo-blue.png"
    }
  }
}
